// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

model Student {
  id Int @id @default(autoincrement())
  course String
  regno String @unique
  name String
  password String
}

model Lecturer {
  id Int @id @default(autoincrement())
  department String
  regno String @unique
  name String
  password String
}

model Results {
  id Int @id @default(autoincrement())
  course String
  regno String
  semester String
  code String
  name String
  mark String
}

model Gpa {
  id Int @id @default(autoincrement())
  course String
  regno String
  semester String
  gpa String
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
